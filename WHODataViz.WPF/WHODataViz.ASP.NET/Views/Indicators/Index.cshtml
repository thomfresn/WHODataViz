@model IEnumerable<WHODataViz.DataModel.Indicator>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Description, "IndicatorData", new { code = item.Code})
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
    </tr>
}

</table>
