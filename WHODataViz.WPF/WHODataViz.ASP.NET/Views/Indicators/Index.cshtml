@model IEnumerable<WHODataViz.DataModel.Indicator>

@{
    ViewBag.Title = "Indicators";
}

<h2>Indicators</h2>

@using(Html.BeginForm("Index", "Indicators", FormMethod.Get)){ 
    @Html.TextBox("q")
    <input type="submit" value="Search"/>
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    Html.RenderPartial("IndicatorItem", item);
}

</table>
